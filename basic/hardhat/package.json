{
  "name": "@nerfzael/nft-test",
  "description": "NFT test",
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "typechain": "hardhat typechain",
    "build": "yarn run clean && yarn run compile && tsc",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "hardhat": "hardhat",
    "test": "hardhat test --show-stack-traces",
    "coverage": "yarn run build && hardhat coverage --temp artifacts --network hardhat",
    "fix": "yarn run fix:contracts && yarn run fix:ts",
    "fix:contracts": "prettier --write ./contracts/**/*.sol",
    "fix:ts": "eslint . --ext .ts --fix",
    "lint": "yarn run lint:contracts && yarn run lint:ts",
    "lint:contracts": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
    "lint:ts": "eslint . --ext .ts"
  },
  "files": [
    "contracts",
    "deployments/*/*.json",
    "deployments/*/.chainid",
    "dist/src/types",
    "dist/deploy/*.js"
  ],
  "dependencies": {},
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "3.0.2",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@openzeppelin/contracts": "4.5.0",
    "@openzeppelin/contracts-upgradeable": "4.5.1",
    "@openzeppelin/hardhat-upgrades": "1.15.0",
    "@openzeppelin/upgrades-core": "1.13.0",
    "@typechain/ethers-v5": "9.0.0",
    "@typechain/hardhat": "4.0.0",
    "@types/chai": "4.3.0",
    "@types/mocha": "9.1.0",
    "@types/node": "16.11.26",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "custom-env": "2.0.1",
    "dotenv": "16.0.0",
    "eslint": "8.10.0",
    "eslint-plugin-prettier": "4.0.0",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.5.4",
    "hardhat": "2.9.0",
    "hardhat-deploy": "0.10.5",
    "hardhat-gas-reporter": "1.0.8",
    "keccak256": "1.0.6",
    "logdown": "3.3.1",
    "merkletreejs": "0.2.30",
    "prettier": "2.5.1",
    "prettier-plugin-solidity": "1.0.0-beta.19",
    "solhint": "3.3.7",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "0.7.20",
    "ts-generator": "0.1.1",
    "ts-node": "10.6.0",
    "typechain": "7.0.0",
    "typescript": "4.6.2"
  }
}
