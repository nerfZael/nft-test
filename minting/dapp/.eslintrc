{
  "root": true,
  "ignorePatterns": [
    "**/node_modules/**/*.*",
    "**/bin/**/*.*",
    "**/build/**/*.*",
    "**/dist/**/*.*",
    "**/coverage/**/*.*",
    "**/node_modules/**/*.*",
    "**/typechain/**/*.*",
    "**/typechain-types/**/*.*"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off"
  }
}
