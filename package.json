{
  "name": "@nerfzael/nft-test",
  "description": "NFT test",
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "typechain": "hardhat typechain",
    "build": "yarn run clean && yarn run compile && tsc",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "hardhat": "hardhat",
    "test": "hardhat test --show-stack-traces",
    "coverage": "yarn run build && hardhat coverage --temp artifacts --network hardhat",
    "fix": "yarn run fix:contracts && yarn run fix:ts",
    "fix:contracts": "prettier --write ./contracts/**/*.sol",
    "fix:ts": "eslint . --ext .ts --fix",
    "lint": "yarn run lint:contracts && yarn run lint:ts",
    "lint:contracts": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
    "lint:ts": "eslint . --ext .ts"
  },
  "files": [
    "contracts",
    "deployments/*/*.json",
    "deployments/*/.chainid",
    "dist/src/types",
    "dist/deploy/*.js"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "hardhat-gas-reporter": "^1.0.4",
    "@ensdomains/ens-contracts": "^0.0.4",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.11",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@openzeppelin/contracts-upgradeable": "4.4.0",
    "@openzeppelin/hardhat-upgrades": "^1.12.0",
    "@openzeppelin/upgrades-core": "^1.10.0",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.1",
    "hardhat": "^2.7.0",
    "hardhat-deploy": "^0.9.12",
    "logdown": "^3.3.1",
    "prettier": "^2.5.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^6.0.5",
    "typescript": "^4.5.2"
  }
}
